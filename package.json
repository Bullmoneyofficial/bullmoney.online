{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "boost": "python3 scripts/boost.py",
    "boost:fast": "python3 scripts/boost.py --skip-heavy",
    "boost:report": "python3 scripts/boost.py --report",
    "dev": "python3 scripts/boost.py --skip-heavy && concurrently -n \"NEXT,CASINO-PHP,CASINO-NODE\" -c \"cyan,yellow,green\" \"node scripts/dev-logger.mjs --turbo\" \"cd Bullcasino && php artisan serve --host=0.0.0.0 --port=8000\" \"cd Bullcasino/server && node app.js\"",
    "dev:no-casino": "python3 scripts/boost.py --skip-heavy && node scripts/dev-logger.mjs --turbo",
    "dev:standard": "python3 scripts/boost.py --skip-heavy && node scripts/dev-logger.mjs",
    "dev:fast": "python3 scripts/boost.py --skip-heavy && NODE_OPTIONS='--max-old-space-size=8192' node scripts/dev-logger.mjs --turbo",
    "dev:webpack": "python3 scripts/boost.py --skip-heavy && node scripts/dev-logger.mjs --webpack",
    "dev:plain": "python3 scripts/boost.py --skip-heavy && NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
    "build": "python3 scripts/boost.py && NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:fast": "python3 scripts/boost.py && NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start -p $PORT",
    "start:fast": "NODE_OPTIONS='--max-old-space-size=4096' next start -p ${PORT:-3000}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo 'Running production readiness checks...' && npm run lint && npm run build",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "check-deps": "node scripts/dependency-checker.mjs",
    "check-deps:update": "node scripts/dependency-checker.mjs --update --fix",
    "check-deps:report": "node scripts/dependency-checker.mjs --report",
    "security": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "pre-deploy": "node scripts/pre-deploy.mjs",
    "deploy-check": "npm run type-check && npm run lint && npm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "casino": "./scripts/start-casino.sh",
    "casino:laravel": "cd Bullcasino && php artisan serve --host=0.0.0.0 --port=8000",
    "casino:socket": "cd Bullcasino/server && node app.js",
    "casino:migrate": "cd Bullcasino && php artisan migrate:fresh --seed",
    "casino:install": "cd Bullcasino && composer install && npm install"
  },
