{
  "crons": [
    {
      "path": "/api/cron/notifications",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/crypto-payment/verify",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/push/poll",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/campaigns",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/email-drip",
      "schedule": "0 */6 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/store",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    },
    {
      "source": "/product/:path*",
      "destination": "/store/product/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    },
    {
      "source": "/checkout",
      "destination": "/store/checkout",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    },
    {
      "source": "/gift-cards",
      "destination": "/store/gift-cards",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    },
    {
      "source": "/account/:path*",
      "destination": "/store/account/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    },
    {
      "source": "/success/:path*",
      "destination": "/store/success/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?:www\\.)?bullmoney\\.shop"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/casino-games",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
