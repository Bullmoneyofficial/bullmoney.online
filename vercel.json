{
  "rewrites": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "bullmoney.shop" }],
      "destination": "/store"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "www.bullmoney.shop" }],
      "destination": "/store"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/notifications",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/crypto-payment/verify",
      "schedule": "*/5 * * * *"
    }
  ]
}
